
@{
    ViewBag.Title = "LSP Direct Material Reports";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section css{
    @Styles.Render("~/LSPDirectMaterial-CSS")
}
<div class="panel panel-inverse" id="main-panel">
    <div class="panel-heading">
        <div class="panel-heading-btn">
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-click="panel-expand">
                <i class="fa fa-expand"></i>
            </a>
        </div>
        <h4 class="panel-title"><span class="fa fa-list-alt"></span>  @ViewBag.Title</h4>
    </div>
    <div class="panel-body">
        <style>
            fieldset {
                border: 1px solid grey;
                padding: 10px;
                border-radius: 5px;
            }

            .input-group .br-r-0, .br-r-0 + .select2 .select2-selection--single {
                border-top-right-radius: 0px !important;
                border-bottom-right-radius: 0px !important;
                border-radius: 0px !important;
                border-right: 0px !important;
            }

                .input-group .br-r-0 + .form-control, .br-r-0 + .select2 + .form-control + .select2 .select2-selection--single {
                    border-top-left-radius: 0px !important;
                    border-bottom-left-radius: 0px !important;
                    border-radius: 0px 4px 4px 0 !important;
                }
        </style>
        <form id="frmReport" action="" method="post" target="_blank">
            <div class="row">
                <div class="col-lg-12 col-xl-12">
                    <fieldset class="m-b-10">
                        <div class="row">
                            <div class="col-sm-3 m-b-15">
                                <div class="checkbox checkbox-css">
                                    <input type="checkbox" data-url="/Reports/LSPDirectMaterial/GenerateFinishedGoodsAndSalesReport" class="ReportType ReportTypeG1" id="FinishedGoodsAndSalesReport" name="FinishedGoodsAndSalesReport" value="Finished Goods and Sales Report">
                                    <label for="FinishedGoodsAndSalesReport">Finished Goods and Sales Report</label>
                                </div>
                            </div>
                            <div class="col-sm-3 m-b-15">
                                <div class="checkbox checkbox-css">
                                    <input type="checkbox" data-url="/Reports/LSPDirectMaterial/GenerateMiscellaneousTransactionReport" class="ReportType ReportTypeG1" id="MiscellaneousTransactionReport" name="MiscellaneousTransactionReport" value="Miscellaneous Transaction Report">
                                    <label for="MiscellaneousTransactionReport">Miscellaneous Transaction Report</label>
                                </div>
                            </div>
                            <div class="col-sm-3 m-b-15">
                                <div class="checkbox checkbox-css">
                                    <input type="checkbox" data-url="/Reports/LSPDirectMaterial/GenerateDMAndLaborPercentageReport" class="ReportType ReportTypeG1" id="DMAndLaborPercentageReport" name="DMAndLaborPercentageReport" value="DM and Labor Percentage Report">
                                    <label for="DMAndLaborPercentageReport">DM and Labor Percentage Report</label>
                                </div>
                            </div>
                            <div class="col-sm-3 m-b-15">
                                <div class="checkbox checkbox-css">
                                    <input type="checkbox" data-url="/Reports/LSPDirectMaterial/GenerateRMBreakdownForFGAndSalesReport" class="ReportType ReportTypeG1" id="RMBreakdownForFGAndSalesReport" name="RMBreakdownForFGAndSalesReport" value="DM and Labor Percentage Report">
                                    <label for="RMBreakdownForFGAndSalesReport">RM Breakdown for FG and Sales Report</label>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-6">
                                <div class="input-group input-group-sm m-b-5">
                                    <div class="input-group-prepend">
                                        <label class="input-group-text" style="width:120px;" for="Start Date"> Start Date <span class="text-danger">*</span></label>
                                    </div>
                                    <input type="text" id="StartDate" name="StartDate" class="form-control input" placeholder="Start Date" autocomplete="off">
                                </div>
                                <div id="err-StartDate"></div>
                            </div>
                            <div class="col-sm-6">
                                <div class="input-group input-group-sm m-b-5">
                                    <div class="input-group-prepend">
                                        <label class="input-group-text" style="width:120px;" for="EndDate"> End Date <span class="text-danger">*</span></label>
                                    </div>
                                    <input type="text" id="EndDate" name="EndDate" class="form-control input" placeholder="End Date" autocomplete="off">
                                </div>
                                <div id="err-EndDate"></div>
                            </div>
                        </div>
                        <div class="p-5">
                            <fieldset>
                                <div class="input-group input-group-sm m-b-5">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" style="width:115px;">Product Code</span>
                                    </div>
                                    <select id="ProductCode1" name="ProductCode1" class="form-control form-control-sm ProductCodes br-r-0"></select>
                                    <select id="ProductCode2" name="ProductCode2" class="form-control form-control-sm ProductCodes"></select>
                                </div>
                                <div class="input-group input-group-sm m-b-5">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" style="width:115px;">Model</span>
                                    </div>
                                    <select id="Model1" name="Model1" class="form-control form-control-sm ProductCodes br-r-0"></select>
                                    <select id="Model2" name="Model2" class="form-control form-control-sm ProductCodes"></select>
                                </div>
                            </fieldset>
                        </div>
                    </fieldset>
                    <fieldset class="m-b-10">
                        <div class="row">
                            <div class="col-sm-6">
                                <div class="checkbox checkbox-css">
                                    <input type="checkbox" data-url="/Reports/LSPDirectMaterial/SlowMonitoringAnalysisReport" class="ReportType ReportTypeG3" id="SlowMonitoringAnalysisReport" name="SlowMonitoringAnalysisReport" value="Slow Monitoring Analysis Report">
                                    <label for="SlowMonitoringAnalysisReport">Slow Monitoring Analysis Report</label>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="input-group input-group-sm m-b-5">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" style="width:115px;">Month <span class="text-danger">*</span></span>
                                    </div>
                                    <select id="Month" name="Month" class="form-control form-control-sm ReportTypeG3" disabled>
                                        <option value="">--Please Select--</option>
                                        <option value="12">12</option>
                                        <option value="24">24</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                    <fieldset class="m-b-10">
                        <div class="row">
                            <div class="col-sm-6">
                                <div class="checkbox checkbox-css">
                                    <input type="checkbox" data-url="/Reports/LSPDirectMaterial/GenerateInventoryTurnoverReport" class="ReportType ReportTypeG6" id="InventoryTurnoverReport" name="InventoryTurnoverReport" value="Inventory Turnover Report">
                                    <label for="InventoryTurnoverReport">Inventory Turnover Report</label>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="checkbox checkbox-css">
                                    <input disabled type="checkbox" id="ShowDetailedTransaction" name="ShowDetailedTransaction" value="Show Detailed Transaction">
                                    <label for="ShowDetailedTransaction">Show Detailed Transactions</label>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                    <fieldset class="m-b-10">
                        <div class="row">
                            <div class="col-sm-6">
                                <div class="checkbox checkbox-css">
                                    <input type="checkbox" data-url="/Reports/LSPDirectMaterial/GenerateWIPShopFloorReport" class="ReportType ReportTypeG2" id="WIPShopFloorReport" name="WIPShopFloorReport" value="Inventory Turnover Report">
                                    <label for="WIPShopFloorReport">WIP Shop Floor Report</label>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                    <fieldset class="m-b-10">
                        <div class="row">
                            <div class="col-sm-4">
                                <div class="checkbox checkbox-css">
                                    <input type="checkbox" data-url="/Reports/LSPDirectMaterial/GenerateRMBreakdownPerJOReport" class="ReportType ReportTypeG4" id="RMBreakdownPerJOReport" name="RMBreakdownPerJOReport" value="Slow Monitoring Analysis Report">
                                    <label for="RMBreakdownPerJOReport">RM Breakdown per JO Report</label>
                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="input-group input-group-sm m-b-5">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" style="width:115px;">PO Number <span class="text-danger">*</span></span>
                                    </div>
                                    <input type="text" id="PONumber" name="PONumber" class="form-control form-control-sm ReportTypeG4" disabled />
                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="input-group input-group-sm m-b-5">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" style="width:115px;">JO Number <span class="text-danger">*</span></span>
                                    </div>
                                    <input type="text" id="JONumber" name="JONumber" class="form-control form-control-sm ReportTypeG4" disabled />
                                </div>
                            </div>
                        </div>
                    </fieldset>
                    <fieldset class="m-b-10">
                        <div class="row">
                            <div class="col-sm-4">
                                <div class="checkbox checkbox-css">
                                    <input type="checkbox" data-url="/Reports/LSPDirectMaterial/GenerateRMBeginningBalanceReport" class="ReportType ReportTypeG5" id="RMBeginningBalanceReport" name="RMBeginningBalanceReport" value="Slow Monitoring Analysis Report">
                                    <label for="RMBeginningBalanceReport">RM Beginning Balance Report</label>
                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="input-group input-group-sm m-b-5">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" style="width:115px;">Product Code <span class="text-danger">*</span></span>
                                    </div>
                                    <select id="ProductCode" name="ProductCode" class="form-control form-control-sm ReportTypeG5" ></select>
                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="input-group input-group-sm m-b-5">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" style="width:115px;">Transaction Date <span class="text-danger">*</span></span>
                                    </div>
                                    <input type="text" id="TransactionDate" name="TransactionDate" class="form-control form-control-sm ReportTypeG5" disabled />
                                </div>
                            </div>
                        </div>
                    </fieldset>
                    <div class="row">
                        <div class="col-sm-6 offset-md-3">
                            <button type="button" class="btn btn-sm btn-success btn-block" id="btnPrint" disabled><span class="fa fa-print"></span> Print</button>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>

@section scripts{
    @Scripts.Render("~/LSPDirectMaterial-JS")
}
